in code;

use std::stream;

pub class CommentCode over CommentNode {

    let _content : [c8];

    pub self (content : [c8])
        with  _content = content
    {}


    pub over dumpHtml (self, dmut stream : &StringStream) {
        stream:.write ("<pre><code class=\"language-ymir\">");
        stream:.write (self._content);
        stream:.write ("</code></pre>");
    }

    impl Streamable;
}
