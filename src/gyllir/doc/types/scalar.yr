in scalar;

use std::{stream, fs::path};

@final
pub class ScalarType over Type {

    pub let name : [c8];

    pub self (name : [c8], mutability : [c8])
        with super (mutability-> mutability)
        , name = name
    {}

    impl Streamable {
        pub over toStream (self, dmut stream : &StringStream) {
            stream:.write ("Scalar (", self.name, ")");
        }
    }

}
