in slice;

use std::stream;

@final
pub class SliceType over Type {

    // The inner type of the slice
    pub let child : &Type;
    
    pub self (isMut : bool, inner : &Type)
        with super (isMut-> isMut)
        , child = inner
    {}

    impl Streamable {
        pub over toStream (self, dmut stream : &StringStream) {
            stream:.write ("Slice (", self.child, ")");
        }
    }

    
}
