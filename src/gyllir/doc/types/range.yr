in range;

use std::stream;

@final
pub class RangeType over Type {

    // The inner type of the range
    pub let child : &Type;
    
    pub self (isMut : bool, inner : &Type)
        with super (isMut-> isMut)
        , child = inner
    {}

    impl Streamable {
        pub over toStream (self, dmut stream : &StringStream) {
            stream:.write ("Range (", self.child, ")");
        }
    }
}
