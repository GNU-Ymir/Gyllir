mod gyllir::doc::types::float;

import gyllir::doc::types::base;
import std::io;

/**
 * Floating point type
 */
pub class FloatType over Type {

    /**
     * @params:
     *    - name: the name of the type (assumed to be f32 or f64)
     *    - isMut: true if the type is mutable
     */
    pub self (name : [c8], isMut : bool)
        with super (name-> name, isMut-> isMut)
    {}


    impl Streamable;
}
